  "private": true,
  "author": "React EPFL",
  "contributors": [
    "Juan Carlos Farah",
    "Basile Spaenlehauer"
  ],
  "homepage": ".",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@graasp/apps-query-client": "github:graasp/graasp-apps-query-client.git",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@monaco-editor/react": "4.2.2",
    "date-fns": "2.25.0",
    "http-status-codes": "2.2.0",
    "i18next": "21.6.11",
    "immutable": "4.0.0",
    "lodash": "4.17.21",
    "prismjs": "1.25.0",
    "prop-types": "15.8.1",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-i18next": "11.15.7",
    "react-mde": "11.5.0",
    "react-redux": "7.2.6",
    "react-scripts": "5.0.0",
    "react-select": "5.2.2",
    "react-toastify": "8.0.2",
    "showdown": "1.9.1"
  },
  "scripts": {
    "start": "yarn start:local",
    "start:local": "env-cmd -f ./.env.local react-app-rewired start",
    "start:test": "env-cmd -f ./.env.test react-scripts -r @cypress/instrument-cra start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "run-s build:react build:bundle",
    "build:react": "react-app-rewired build",
    "build:bundle": "webpack --config webpack.config.js --mode=production",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "eject": "react-scripts eject",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently \"yarn start:test\" \"wait-on http://localhost:3333 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.spec.js\""
  },
  "devDependencies": {
    "@babel/core": "7.17.7",
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.16.5",
    "@babel/plugin-syntax-flow": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@cypress/code-coverage": "3.9.12",
    "@cypress/instrument-cra": "1.4.0",
    "babel-plugin-prismjs": "2.1.0",
    "codacy-coverage": "3.4.0",
    "concurrently": "7.0.0",
    "cross-env": "7.0.3",
    "customize-cra": "1.0.0",
    "cypress": "9.5.2",
    "env-cmd": "10.1.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "glob": "7.1.7",
    "husky": "7.0.4",
    "json-server": "0.16.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.0",
    "pretty-quick": "3.1.1",
    "react-app-rewired": "2.1.8",
    "standard-version": "9.3.1",
    "typescript": "4.6.2"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.2.0",
  "peerDependencies": {
    "eslint": "*",
    "eslint-plugin-react-hooks": "*",
    "monaco-editor": "*"
  }
}
